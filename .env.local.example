# OnchainKit API Key (required for wallet and identity features)
NEXT_PUBLIC_ONCHAINKIT_API_KEY=your_api_key_here

# WalletConnect Project ID (optional, for WalletConnect support)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_project_id_here

# Payment API URL (optional, defaults to /api/payments)
NEXT_PUBLIC_PAYMENT_API_URL=http://localhost:3000/api/payments

# Base network RPC URL (optional, uses default if not set)
NEXT_PUBLIC_BASE_RPC_URL=https://mainnet.base.org